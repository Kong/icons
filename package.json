{
  "name": "@kong/icons",
  "version": "0.0.1",
  "description": "Kong Icon Library",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/kong-icons.umd.js",
  "module": "./dist/kong-icons.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "typings": "dist/types",
  "exports": {
    ".": {
      "import": "./dist/kong-icons.es.js",
      "require": "./dist/kong-icons.umd.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn stylelint && yarn lint && yarn build:icons && yarn docs:build",
    "build:ci": "yarn build:icons && yarn docs:build",
    "build:components": "rimraf ./dist && node './src/component-generator' && yarn typecheck && vite build && vue-tsc -p './tsconfig.build.json' --emitDeclarationOnly && tsc-alias -p './tsconfig.build.json'",
    "build:visualize": "BUILD_VISUALIZER=true rimraf ./dist && node './src/component-generator' && vite build -m production",
    "dev": "USE_SANDBOX=true vite",
    "docs:build": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:serve": "vitepress serve docs",
    "docs:preview": "vitepress preview docs --port 8080",
    "typecheck": "vue-tsc -p './tsconfig.build.json' --noEmit",
    "lint": "eslint '**/*.{js,ts,vue}'",
    "lint:fix": "eslint '**/*.{js,ts,vue}' --fix",
    "stylelint": "stylelint './src/**/*.{css,scss,vue}'",
    "stylelint:fix": "stylelint './src/**/*.{css,scss,vue}' --fix",
    "semantic-release": "semantic-release",
    "commit": "cz"
  },
  "devDependencies": {
    "@babel/types": "^7.22.5",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@digitalroute/cz-conventional-changelog-for-jira": "^8.0.1",
    "@evilmartians/lefthook": "^1.4.3",
    "@kong/design-tokens": "1.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^20.4.0",
    "@types/node-emoji": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "cheerio": "^1.0.0-rc.12",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.15.1",
    "node-emoji": "^2.1.0",
    "picocolors": "^1.0.0",
    "postcss-html": "^1.5.0",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.63.6",
    "semantic-release": "^21.0.7",
    "stylelint": "^15.10.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^6.0.3",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.1",
    "vitepress": "^1.0.0-beta.5",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.4"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "homepage": "https://icons.konghq.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kong/icons.git"
  },
  "author": {
    "name": "Kong Inc.",
    "url": "https://konghq.com/"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">=16.19.0"
  },
  "volta": {
    "node": "18.16.1",
    "yarn": "1.22.17"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "skipScope": false,
      "jiraOptional": true,
      "jiraLocation": "post-description",
      "jiraPrepend": "[",
      "jiraAppend": "]"
    }
  }
}
